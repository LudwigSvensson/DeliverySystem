@model DriverInfo.Models.Employee

<h2>Create New Employee</h2>
<form asp-action="Create" method="post">
    <label>Email</label>
    <input asp-for="Email" />
    <label>Name</label>
    <input asp-for="Name" />
    <label>Password</label>
    <input type="password" name="password" />
    <button type="submit">Create</button>
</form>
